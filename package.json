    "@parcel/transformer-sass": "^2.16.0",
    "autoprefixer": "^10.4.0",
    "core-js": "^3.30.0",
    "parcel": "^2.9.0",
    "postcss": "^8.4.0",
    "sass": "^1.60.0"
  }
